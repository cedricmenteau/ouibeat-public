<h3>Mise à jour paragraphe détail</h3>
<div class="container">
  <div class="row">
    <div class="col s10 offset-s1">
      <%= simple_form_for [@project, @detail] do |f| %>
        <%= f.input :title %>
        <%= f.input :text %>
        <%= f.input :link %>
        <%= f.input :pictures, as: :attachinary %>
        <%= f.submit "Sauvegarder", class: "waves-effect waves-light btn" %>
      <% end %>
    </div>
  </div>
</div>


