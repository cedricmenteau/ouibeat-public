<div class="container">
  <ul class="collection">
    <% @logos.each do |logo| %>
      <li class="collection-item">
        <span class="title"><%= logo.name %></span>
        <%= cl_image_tag logo.picture.url %>
        <span class="position"><%= logo.position %></span>
      </li>
    <% end %>
  </ul>
  <!-- NEW LOGO -->
    <%= link_to new_admin_logo_path do %>
      <p class="btn-floating btn-large waves-effect waves-light red"><i class="material-icons">add</i></p>
    <% end %>
</div>
