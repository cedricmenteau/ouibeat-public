<% if @logos.present? %>
  $('.logo-container').slice(-1).append('<%= j render partial: "shared/section_logos", locals: {logos: @logos[0..11]} %>')
  $('.logos').slideDown();
  <% if params[:position] == nil %>
    $('.btn-more').addClass('hidden');
    $('.btn-more').attr('href', "#")
  <% else %>
   var href = $('.btn-more').attr('href').split('=')[0] + "=" + <%= params[:position] %>
    $('.btn-more').attr('href', href)
  <% end %>
<% end %>
